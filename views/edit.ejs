
    <div class="container my-5">
        <div class="row justify-content-center">
            <div class="col-md-5">
                <a class="btn btn-sm btn-primary" href="/student">Back</a>
                <br>
                <br>
                <div class="card">
                    <div class="card-body">
                        <h2>Edit data  student</h2>
                        <hr>
                  
                        <form action="/student/edit/<%= editStudent.id %>" method="post" enctype="multipart/form-data">
                            <div class="my-3">
                                <label  for="">Name</label>
                                <input value="<%= editStudent.name %>" name="name" class="form-control" type="text">
                            </div>
                            <div class="my-3">
                                <label for="">Email</label>
                                <input value="<%= editStudent.email%>" name="email" class="form-control" type="text">
                            </div>
                            <div class="my-3">
                                <label for="">Cell</label>
                                <input value="<%= editStudent.cell %>" name="cell" class="form-control" type="text">
                            </div>
                            <input type="hidden" value="<%= editStudent.photo %>" name="old_photo">

                            <div class="my-3">
                                <label for="">Photo</label>
                                <input  name="photo" value="<%= editStudent.photo %>" class="" type="file">
                               
                            </div>

                          

                            <div class="my-3">
                                <input  type="submit" value="submit" class="btn btn-primary">
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>


