
    <div class="container my-5">
        <div class="row justify-content-center">
            <div class="col-md-8">
                <a class="btn btn-sm btn-primary" href="/student/create">Add New Student</a>
                <br>
                <br>
                <div class="card">
                    <div class="card-body">
                        <h2>all student</h2>
                        <hr>
                        <table class="table table-stiped">
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th>Cell</th>
                                    <th>Photo</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                       <% students.forEach((element , index) => { %>
                        
                        <tr>
                            <td>01</td>
                            <td><%= element.name %> </td>
                            <td><%= element.email %></td>
                            <td><%= element.cell %></td>
                            <td><img style="width: 50px; height: 50px; object-fit : cover ;" src="assets/upload/<%= element.photo %>" alt=""></td>
                            <td>
                                <a class="btn btn-sm btn-info" href="/student/<%= element._id %>"> <i class=" fas fa-eye"></i></a>
                                <a class="btn btn-sm btn-warning" href="/student/edit/<%= element._id %>"> <i class=" fas fa-edit"></i></a>
                                <a class="btn btn-sm btn-danger" href="/student/delete/<%= element._id %>"> <i class=" fas fa-trash"></i></a>
                            </td>
                        </tr>

                       <% }) %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>


